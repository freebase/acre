[
    {
        "host": null,
        "rules":[
            {
                "from":{
                    "param":{
                        "key":"acre.console",
                        "value":"1"
                    }
                },
                "to":{
                    "route_as":"script",
                    "location":"/console/console"
                }
            },
            {
                "from":{
                    "path":["/status", "/acre/status"]
                },
                "to":{
                    "route_as":"script",
                    "location":"/status/status"
                }
            },
            {
                "from":{
                    "path":"/acre/version"
                },
                "to":{
                    "route_as":"script",
                    "location":"/status/version"
                }
            },
            {
                "from":{
                    "path":["/api/service/touch", "/acre/touch"]
                },
                "to":{
                    "route_as":"servlet",
                    "servlet":"ProxyPassServlet",
                    "path":"/api/service/touch"
                }

            },
            {
                "from":{
                    "path":"/acre/account"
                },
                "to":{
                    "route_as":"script",
                    "location":"/account/account"
                }
            },
            {
                "from":{
                    "path":"/acre/keystore"
                },
                "to":{
                    "route_as":"servlet",
                    "servlet":"KeyStoreServlet",
                    "path":"/"
                }
            },
            {
                "from":{
                    "path":"/acre/logviewer"
                },
                "to":{
                    "route_as":"script",
                    "location":"/console/logviewer"
                }
            },
            {
                "from":{
                    "path":"/acre/form"
                },
                "to":{
                    "route_as":"script",
                    "location":"/freebase/apps/forms/fbedit"
                }
            },
            {
                "from":{
                    "path":"/acre/static"
                },
                "to":{
                    "route_as":"static",
                    "path":"/global_static"
                }
            },
            {
                "from":{
                    "path":"/acre"
                },
                "to":{
                    "route_as":"app",
                    "location":null
                }
            },
            {
                "from":{
                    "path":"/"
                },
                "to":{
                    "route_as":"app",
                    "location":"/app"
                }
            }
        ]
    }
]
