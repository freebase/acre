[
    {
        "host":null,
        "rules":[
            {
                "from":{
                    "param":{
                        "key":"acre.console",
                        "value":"1"
                    }
                },
                "to":{
                    "route_as":"script",
                    "location":"/console/console"
                }
            },
            {
                "from":{
                    "param":{
                        "key":"acre.source",
                        "value":"1"
                    }
                },
                "to":{
                    "route_as":"script",
                    "location":"/local/source"
                }
            },
            {
                "from":{
                    "path":["/status", "/acre/status"]
                },
                "to":{
                    "route_as":"script",
                    "location":"/local/status"
                }
            },
            {
                "from":{
                    "path":"/acre/test"
                },
                "to":{
                    "route_as":"script",
                    "location":"/test/dashboard"
                }
            },
            {
                "from":{
                    "path":"/acre/version"
                },
                "to":{
                    "route_as":"script",
                    "location":"/local/version"
                }
            },
            {
                "from":{
                    "path":"/acre/touch"
                },
                "to":{
                    "route_as":"script",
                    "location":"/local/touch"
                }
            },
            {
                "from":{
                    "path":["/api/service/touch"]
                },
                "to":{
                    "route_as":"servlet",
                    "servlet":"ProxyPassServlet",
                    "path":"/api/service/touch"
                }

            },
            {
                "from":{
                    "path":"/acre/account"
                },
                "to":{
                    "route_as":"script",
                    "location":"/account/account"
                }
            },
            {
                "from":{
                    "path":"/acre/keystore"
                },
                "to":{
                    "route_as":"servlet",
                    "servlet":"KeyStoreServlet",
                    "path":"/"
                }
            },
            {
                "from":{
                    "path":"/acre/logviewer"
                },
                "to":{
                    "route_as":"script",
                    "location":"/console/logviewer"
                }
            },
            {
                "from":{
                    "path":"/acre/form"
                },
                "to":{
                    "route_as":"app",
                    "location":"/freebase/apps/forms"
                }
            },
            {
                "from":{
                    "path":"/acre/static"
                },
                "to":{
                    "route_as":"static",
                    "path":"/global_static"
                }
            },
            {
                "from":{
                    "path":"/acre/keystore"
                },
                "to":{
                    "route_as":"servlet",
                    "servlet":"KeyStoreServlet",
                    "path":"/"
                }
            }

        ]
    }
]
